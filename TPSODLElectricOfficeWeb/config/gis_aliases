# TPSODL Electric Office Web Vert.X CLI
tpsodl_eo_web_vertx_closed:
  title   = Run TPSODL Electric Office Web Vert.x closed image
  session = tpsodl_electric_office_web:tpsodl_eo_web_vertx_closed
  CONFIG_FOR_STARTUP = magikCLIConfig
  product = tpsodl_electric_office_web

tpsodl_eo_web_vertx_open:
  title   = Run Electric Office Web Vert.x open image
  session = tpsodl_electric_office_web:tpsodl_eo_web_vertx_open
  CONFIG_FOR_STARTUP = magikCLIConfig
  product = tpsodl_electric_office_web

