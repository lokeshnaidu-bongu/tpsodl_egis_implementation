#
# Smallworld Core Windows Platforms Standard Core Product Aliases
#
# The Smallworld Product's standard aliases file should not be edited
# by hand.  A user can have personal aliases by placing them in a file
# named `gis_aliases' in his/her home directory, but the alias names
# must be chosen not to clash with those in the standard aliases file.
#

eo_suite:
	title                   = Start Smallworld Electric Office Product Suite Closed
	session                 = sw_electric_office:eo_suite_closed
	SW_CONSTRUCTION_PACK_DIR= C:/Temp
	SW_LAYOUT_DOCUMENT_DIR  = C:/Temp
	product                 = sw_electric_office
	args		        = -cli
	SW_LAUNCH_JAVA_MEMORY   = -Xmx3000m	

eo_suite_open:
	title                   = Start Smallworld Electric Office Product Suite
	session                 = sw_electric_office:eo_suite_open
	SW_ACE_DB_DIR           = %SMALLWORLD_GIS%/../electric_office/example_db/ds/ds_admin
	SW_CONSTRUCTION_PACK_DIR= C:/Temp
	SW_LAYOUT_DOCUMENT_DIR  = C:/Temp
	splash_screen           = %SMALLWORLD_GIS%/../electric_office/resources/base/bitmaps/smallworld_electric_splash.png
	product                 = sw_electric_office
	args		        = -cli
	SW_LAUNCH_JAVA_MEMORY   = -Xmx3000m

tp_emacs:
    EMACSROOT = %SMALLWORLD_GIS%\..\emacs
    title = TPSODL EO 5 eMacs
    program = %EMACSROOT%\bin\runemacs.exe
    icon_file = %EMACSROOT%\bin\runemacs.exe
    log_file = nul:
    product = emacs
    EMACSLOADPATH = %EMACSROOT%\site-lisp;%EMACSROOT%\lisp;%EMACSROOT%\leim
    PATH = %PATH%;%EMACSROOT%\bin
    HOME = %HOMEDRIVE%%HOMEPATH%
tp_open_emacs:
	EMACSROOT		= %SMALLWORLD_GIS%\..\emacs
	title			= CSPDCL EO 5 eMacs
	program			= %EMACSROOT%\bin\runemacs.exe
	icon_file		= %EMACSROOT%\bin\runemacs.exe
	log_file		= nul:
	product			= emacs
	GIS_COMMAND		= %SMALLWORLD_GIS%\bin\x86\runalias.exe -a %TP_CUSTOM_CONFIG_DIR%gis_aliases tp_custom_open -cli
	EMACSLOADPATH	= %EMACSROOT%\site-lisp;%EMACSROOT%\lisp;%EMACSROOT%\leim
	PATH			= %PATH%;%EMACSROOT%\bin
	HOME			= %HOMEDRIVE%%HOMEPATH%

